<UserControl x:Class="SeeShells.UI.Templates.Switch"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SeeShells.UI.Templates"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Menu Background="Transparent">
            <MenuItem Header="View" Foreground="DarkGray" >
                <Hyperlink NavigateUri="/SeeShells;component/ui/pages/home.xaml" Foreground="Black">Home</Hyperlink>
                <MenuItem x:Name="timeline" Header="Timeline Page" Click="MenuItem_Click" Foreground="Black" />
            </MenuItem>
            <MenuItem Header="Help" Foreground="DarkGray">
                <Hyperlink NavigateUri="/SeeShells;component/ui/pages/helppage.xaml" Foreground="Black" >Help</Hyperlink>
                <Hyperlink Foreground="Black">About</Hyperlink>
            </MenuItem>
        </Menu>
    </Grid>
</UserControl>
