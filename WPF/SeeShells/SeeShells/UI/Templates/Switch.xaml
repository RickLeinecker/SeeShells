<UserControl x:Class="SeeShells.UI.Templates.Switch"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:SeeShells.UI.Templates"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Menu Background="Transparent">
            <MenuItem Header="View" Foreground="DarkGray" >
                <MenuItem Header="Home" Click="Home_Click" Foreground="Black"/>
                <MenuItem x:Name="timeline" Header="Timeline Page" Click="MenuItem_Click" Foreground="Black" />
            </MenuItem>
            <MenuItem Header="Help" Foreground="DarkGray">
                <MenuItem Header="Help page" Click="Help_Click" Foreground="Black"/>
                <MenuItem Header="About" Click="About_OnClick" Foreground="Black"/>
            </MenuItem>
            <MenuItem x:Name="csvimport" Header="Import CSV" Click="Import_Click" Foreground="DarkGray"/>
        </Menu>
    </Grid>
</UserControl>
