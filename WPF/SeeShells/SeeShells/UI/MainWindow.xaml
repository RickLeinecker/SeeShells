<Window x:Class="SeeShells.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SeeShells"
        xmlns:template="clr-namespace:SeeShells.UI.Templates"
        mc:Ignorable="d"
        Title="SeeShells" Height="500" Width="850"
        MinHeight="500" MinWidth="850"
        Loaded="Window_Loaded"
        >
    <Window.RenderTransform>
        <ScaleTransform ScaleX="{Binding ZoomX}" ScaleY="{Binding ZoomY}"/>
    </Window.RenderTransform>

    <Grid>
        <Frame x:Name="mainframe" NavigationUIVisibility="Hidden">
        </Frame>
    </Grid>
</Window>
