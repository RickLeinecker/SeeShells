<Window x:Class="SeeShells.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SeeShells"
        mc:Ignorable="d"
        Title="SeeShells" Height="450" Width="800">
    <Window.RenderTransform>
        <ScaleTransform ScaleX="{Binding ZoomX}" ScaleY="{Binding ZoomY}"/>
    </Window.RenderTransform>

    <Grid>
        <Frame NavigationUIVisibility="Hidden" Source="Pages/Home.xaml">
        </Frame>
    </Grid>
</Window>
